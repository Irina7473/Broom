create table tab_types_of_events
(
    id integer not null primary key autoincrement,
    type_event text not null
);

create table tab_total_log
(
    id integer not null primary key autoincrement ,
    id_type_event integer not null ,
    date_time_event text not null,
    user text not null,
    message text not null
);

create table tab_success_log
(
    id integer not null primary key autoincrement,
    id_type_event integer not null ,
    date_time_event text not null,
    user text not null,
    success_message text not null
);

create table tab_errors_log
(
    id integer not null primary key autoincrement,
    id_type_event integer not null ,
    date_time_event text not null,
    user text not null,
    errors_message text not null
);

